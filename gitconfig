[user]
name = Brandon Chinn
email = brandon@leapyear.io

[rebase]
autoSquash = true

[push]
default = current

[alias]
fixup = commit --fixup
squash = commit --amend --no-edit
touch = commit --amend -C HEAD --date now --no-verify --allow-empty
l = log --oneline -10
s = status -sb
c = checkout

[merge]
conflictstyle = diff3
