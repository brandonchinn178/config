#compdef git-bb

function _git_bb {
    emulate -L zsh
    local flags=(
        --list
        --base
        --deps
        --default-base-branch
        --display
        --rename-to
        --set-base
        --add-dep
        --rm-dep
        --clear
    )
    __gitcomp "${flags}"

    __gitcomp_direct "$(__git_heads)"
}
